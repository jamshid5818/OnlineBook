<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.OnlineBook" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#27B26C</item>
        <item name="colorPrimaryVariant">#83E6B4</item>
        <item name="colorOnPrimary">@color/black</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

        <item name="titleTextColor">@color/black</item>

    </style>

    <style name="bottom_nav">
        <item name="android:background">@color/white</item>
    </style>

    <style name="email">
        <item name="android:textColor">#787777</item>
    </style>

    <style name="texts">
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="bg_layout">
        <item name="android:background">#FCFAEF</item>
    </style>

    <style name="profile_name_layout">
        <item name="android:background">@drawable/bg_name</item>
    </style>


</resources>